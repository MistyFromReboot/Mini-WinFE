[main]
Title=JkDefrag
Description=Commandline disk and file defragmenter - added to right-click context menu.
Selected=False
Level=5
Author=ThePEBakeryTeam
Version=20190426
Date=2019-04-26
Interface=Interface

[Interface]
pWebLabel1=http://www.kessels.com/jkdefrag/,1,10,51,100,216,18,http://www.kessels.com/jkdefrag/
pTextLabel1="JkDefrag Website",1,1,51,77,230,18,10,Bold
pCheckBoxInWim="Include Program in boot.wim",1,3,51,265,180,18,True

[process]
If,%FullProjectRun%,Equal,NO,Run,"%ProjectDir%\Core\common.script",PostConfigRun
If,%pCheckBoxInWim%,Equal,True,Set,%PROGRAMS.IN.WIM%,YES
Else,Set,%PROGRAMS.IN.WIM%,NO
Run,%ScriptFile%,CHECKS
Run,%ScriptFile%,COMMON
If,%PROGRAMS.IN.WIM%,Equal,NO,Run,%ScriptFile%,NOT.IN.WIM
If,%PROGRAMS.IN.WIM%,Equal,YES,Run,%ScriptFile%,IN.WIM

[CHECKS]
If,Not,ExistFile,"%Programs%\JkDefrag\%ARCH%\JkDefrag.drive.from.context.menu.cmd",Begin
Message,JkDefrag is missing - exiting script...,Error,5
Exit,JkDefrag is missing - exiting script...,WARN
End

[COMMON]
If,%PROGRAMS.IN.WIM%,Equal,NO,Set,%PATH%,%OutputDir%
If,%PROGRAMS.IN.WIM%,Equal,YES,Set,%PATH%,%TargetDir%
If,Not,ExistDir,"%PATH%\Programs",DirMake,"%PATH%\Programs"
DirMake,"%PATH%\Programs\JkDefrag"
DirCopy,"%Programs%\JkDefrag\%ARCH%\*.*","%PATH%\Programs\JkDefrag"
If,Not,ExistFile,"%PATH%\Programs\registry.entries.cmd",Begin
FileCreateBlank,"%PATH%\Programs\registry.entries.cmd"
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",@Echo off,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",,Append
End
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",::==========JkDefrag============,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",::for context menu entries,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",IF EXIST #$q#$p~dp0JkDefrag\JkDefrag.cmd#$q (,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",ECHO.,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",ECHO JkDefrag Registry Entries,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",reg.exe add #$qHKLM\SOFTWARE\Classes\*\shell\Defragment#$q /ve /t REG_SZ /d #$qDefragment#$q /f,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",reg.exe add #$qHKLM\SOFTWARE\Classes\*\shell\Defragment\command#$q /ve /t REG_SZ /d #$q\#$q#$p~dp0JkDefrag\JkDefrag.file.from.context.menu.cmd\#$q \#$q%%1\#$q#$q /f,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",reg.exe add #$qHKLM\SOFTWARE\Classes\Drive\shell\Defragment#$q /ve /t REG_SZ /d #$qDefragment#$q,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",reg.exe add #$qHKLM\SOFTWARE\Classes\Drive\shell\Defragment\command#$q /ve /t REG_SZ /d #$q\#$q#$p~dp0JkDefrag\JkDefrag.drive.from.context.menu.cmd\#$q %%1#$q /f,Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",),Append
TXTaddLine,"%PATH%\Programs\registry.entries.cmd",,Append
IniWrite,"%OutputDir%\Project.Settings.ini",PROGRAMS,JkDefrag,YES

[IN.WIM]
Echo,"Mounting registry hives..."
If,Not,ExistRegSection,HKLM,_WinPE_SOFTWARE,RegHiveLoad,_WinPE_SOFTWARE,"%Target_config%\software"
RegWrite,HKLM,0x1,_WinPE_SOFTWARE\Classes\*\shell\Defragment,,Defragment
RegWrite,HKLM,0x2,_WinPE_SOFTWARE\Classes\*\shell\Defragment\command,,#$psystemdrive#$p\Programs\JkDefrag\JkDefrag.file.from.context.menu.cmd #$q%1#$q
RegWrite,HKLM,0x1,_WinPE_SOFTWARE\Classes\Drive\shell\Defragment,,Defragment
RegWrite,HKLM,0x2,_WinPE_SOFTWARE\Classes\Drive\shell\Defragment\command,,#$psystemdrive#$p\Programs\JkDefrag\JkDefrag.drive.from.context.menu.cmd %1
Echo,"Unmounting registry hives..."
RegHiveUnload,_WinPE_SOFTWARE
If,%FullProjectRun%,Equal,NO,Begin
  IniRead,"%TargetDir%\Project.Settings.ini",Main,Image.Number,%Boot.wim.Image.number%
  WimPathAdd,%OutputDir%\sources\boot.wim,%Boot.wim.Image.number%,%TargetDir%,""
End

[NOT.IN.WIM]
If,%FullProjectRun%,Equal,NO,Run,"%ProjectDir%\Core\common.script",PostConfigRunNotInWim

[AuthorEncoded]
logo=JkDefrag.ico
JkDefrag.ico=9925,13234

[EncodedFile-AuthorEncoded-JkDefrag.ico]
lines=3





